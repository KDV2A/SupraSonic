<Window
    x:Class="SupraSonicWin.MainWindow"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:SupraSonicWin"
    Title="SupraSonic Windows">

    <NavigationView x:Name="NavView" SelectionChanged="OnNavSelectionChanged" IsBackButtonVisible="Collapsed">
        <NavigationView.MenuItems>
            <NavigationViewItem Content="Settings" Icon="Setting" Tag="settings"/>
            <NavigationViewItem Content="History" Icon="History" Tag="history"/>
        </NavigationView.MenuItems>

        <Frame x:Name="ContentFrame"/>
    </NavigationView>
</Window>
